{
    "name": "ronnie/framewire",
    "description": "An open source PHP learning framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vicent Simon",
            "email": "vicsimon.vs4@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Framewire\\": "src/",
            "App\\": "app/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^v6.3.8",
        "ext-sockets": "*",
        "php-http/discovery": "^1.19",
        "ext-fileinfo": "*",
        "nette/php-generator": "^4.1"
    },
    "require": {
        "php": "^8.2",
        "symfony/http-kernel": "^v6.3.8",
        "symfony/http-foundation": "^v6.3.8",
        "vlucas/valitron": "^v1.4.11",
        "vlucas/phpdotenv": "^v5.6.0",
        "league/container": "^4.2",
        "league/config": "^1.2",
        "aura/router": "^3.3",
        "symfony/psr-http-message-bridge": "^7.0",
        "nyholm/psr7": "^1.8",
        "roave/better-reflection": "^6.20",
        "crell/tukio": "^1.6",
        "monolog/monolog": "^3.5",
        "league/flysystem": "^3.0",
        "latte/latte": "^3.0",
        "tracy/tracy": "^2.10",
        "oscarotero/env": "^2.1",
        "ronnie/framewire-inertia": "^0.2.0",
        "symfony/asset": "^7.0",
        "cycle/orm": "^2.6",
        "cycle/annotated": "^4.0",
        "symfony/console": "^7.0"
    },
    "suggest": {},
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
